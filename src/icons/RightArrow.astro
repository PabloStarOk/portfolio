---
interface Props {
  size?: number;
  pathStyle?: string;
}

const { size, pathStyle } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  viewBox="0 0 16 16"
  width={size}
  height={size}
>
  <path
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="1.6"
    d="M3.333 8h9.334m0 0L8 3.333M12.667 8 8 12.667"
    style={pathStyle}
  >
  </path>
</svg>
